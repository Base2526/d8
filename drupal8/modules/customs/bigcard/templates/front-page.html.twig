                  <div class="col-lg-9 col-md-9 col-sm-12 banner-left">  
                      <div class="wrap-bannerads">
                        
                            {# carouse #}
                            <div class="banner-carousel">
                              <div id="myCarousel" class="carousel slide" data-ride="carousel">
